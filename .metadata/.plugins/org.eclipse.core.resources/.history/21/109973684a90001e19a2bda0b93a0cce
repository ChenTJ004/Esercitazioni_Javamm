int numMax(int[] a) {
	int max = 0;
	for (int i = 0; i < a.length; i++) {
		if (a[i] > max) {
			max = a[i];
		}
	}
	return max;
}

int [] occorrenzeCompresso(int [] a) {
	int[] occorrenze = new int[numMax(a)];
	for (int i = 0; i < a.length; i++) {
		for (int j = 0; j < a.length; j++) {
		}
	}
	return null;
}
