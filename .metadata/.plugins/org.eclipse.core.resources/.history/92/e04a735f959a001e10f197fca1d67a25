int[][] generaSingolaMatrice(int n, int start){
	int[][] M = new int[n][n];
	for(int i=0; i<n; i++){
		for(int j=0; j<n; j++){
			M[i][j] = start++;
		}
	}
	return M;
}


// Consegna obbligatoria (soluzione iterativa)
